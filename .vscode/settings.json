{
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ no GS",
            "tools": [
                // "latexmk",
                "latexmk"
            ]
        },
        {
            "name": "biber only",
            "tools": [
                "biber"
            ]
        },
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk",
                "latexmk",
                "ghostscript"
            ]
        },
        {
            "name": "latexmk âžž biber âžž latexmkâ€ŠÃ—â€Š2",
            "tools": [
                "latexmk",
                "biber",
                "makeindex",
                "latexmk",
                "latexmk",
                "ghostscript"
            ]
        },
        {
            "name": "compress pdf",
            "tools": [
                "ghostscript"
            ]
        },
        {
            "name": "latexmk debug mode",
            "tools": [
                "latexmk-debugmode"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "ghostscript",
            "command": "gswin64c.exe",
            "args": [
                "-sDEVICE=pdfwrite",
                "-dCompatibilityLevel=1.5",
                "-dPDFSETTINGS=/printer",
                "-dQUIET",
                "-o%DOCFILE%_compressed.pdf",
                "%OUTDIR%/%DOCFILE%.pdf"
            ],
            "env": {}
        },
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "-auxdir=%OUTDIR%/buildfiles",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk-debugmode",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-h",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "-auxdir=%OUTDIR%/buildfiles",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "makeindex",
            "command": "makeindex",
            "args": [
                "%DOCFILE%.nlo",
                "-s",
                "nomentbl.ist",
                "-o",
                "%DOCFILE%.nls",
                "%DOCFILE%.nlo"
            ]
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
                "%DOCFILE%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%/buildfiles"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
    ],
    "ltex.language": "de-DE",
    "editor.formatOnSave": true
}